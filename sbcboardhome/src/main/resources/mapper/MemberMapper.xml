<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.sbcboardhome.mapper.MemberMapper">

    <insert id="signUp" parameterType="com.example.sbcboardhome.model.Member">
        INSERT INTO member (user_id, password, user_name)
        VALUES (#{userId}, #{password}, #{userName})
    </insert>

</mapper>