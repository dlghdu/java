<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="org.example.tobi.springbootbasic.mapper.UserMapper">

    <select id="somefind" resultType="org.example.tobi.sbcnotice.model.User">
